{
    "version": 1,
    "handlers": {
        "neon": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "neon"
        },
        "root": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "root"
        }
    },
    "formatters": {
        "neon": {
            "format": "%(asctime)23s %(levelname)8s %(process)6d:%(threadName)-10s %(name)15s:%(class)-30s %(message)s",
            "style": "%"
        },
        "root": {
            "format": "%(asctime)23s %(levelname)8s %(process)6d:%(threadName)-10s %(name)15s:undefined                      %(message)s",
            "style": "%"
        }
    },
    "loggers": {
        "neon": {
            "level": "DEBUG",
            "handlers": ["neon"],
            "propagate": true
        },
        "": {
            "level": "DEBUG",
            "handlers": ["root"],
            "propagate": false
        }
    }
}
